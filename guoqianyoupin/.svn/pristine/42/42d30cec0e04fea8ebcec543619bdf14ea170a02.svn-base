.ui-align {
  text-align: center;
}
textarea {
  resize: none;
}
.float-right {
  float: right;
}
.float-left {
  float: left;
}
.nav-toolbar {
  width: 100vw;
  height: 16vw;
  background: #fff;
  box-shadow: 0vw 0vw 3.2vw 0vw rgba(26, 17, 18, 0.09);
  z-index: 8;
}
.nav-toolbar .tab {
  text-align: center;
  flex: 1;
}
.nav-toolbar .tab.active {
  color: #2CC7BC;
}
.nav-toolbar .icon-wrapper {
  position: relative;
  padding: 3.2vw 0 1.3333vw;
}
.nav-toolbar .icon-wrapper .icon {
  font-size: 6.1333vw;
}
.nav-toolbar .icon-wrapper .number {
  position: absolute;
  right: 25%;
  top: 14%;
  display: inline-block;
  min-width: 4vw;
  min-height: 4vw;
  padding: 0.6667vw;
  text-align: center;
  line-height: 2.8vw;
  font-size: 2.1333vw;
  color: #fff;
  border-radius: 100%;
  background-color: #C50303;
}
.nav-toolbar .label {
  font-size: 3.4667vw;
}
.top-toolbar {
  position: relative;
  height: 12vw;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.top-toolbar .back-btn {
  position: absolute;
  left: 0;
  top: 0;
}
.top-toolbar .back-btn a {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 12vw;
  height: 12vw;
}
.top-toolbar .back-btn a i {
  font-size: 4.5333vw;
}
.top-toolbar .page-title {
  font-size: 4.8vw;
}
.top-toolbar .default-link {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  position: absolute;
  right: 4vw;
  top: 0;
  font-size: 3.7333vw;
}
.tab-wrapper {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
}
.tab-wrapper .tab-nav {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  align-items: center;
  height: 7.4667vw;
  background: #fff;
}
.tab-wrapper .tab-btn {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  flex: 1;
  height: 100%;
}
.tab-wrapper .tab-btn.active {
  color: #24C3BE;
}
.tab-wrapper .tab-btn.active span {
  position: relative;
}
.tab-wrapper .tab-btn.active span::before {
  content: "";
  display: inline-block;
  position: absolute;
  width: 100%;
  height: 0.5333vw;
  background: #24C3BE;
  bottom: 0;
  left: 0;
}
.label-title {
  font-size: 0.35rem;
  text-align: center;
}
.label-title:after,
.label-title:before {
  display: inline-block;
  content: '';
  width: 2.0rem;
  max-width: 100px;
  height: 1px;
  vertical-align: middle;
  background-color: #999;
}
.label-title:before {
  margin-right: 15px;
}
.label-title:after {
  margin-left: 15px;
}
.num-bar {
  font-size: 0;
}
.num-bar .num-btn {
  display: inline-block;
  vertical-align: middle;
  width: 0.8rem;
  height: 0.8rem;
  font-size: 0.35rem;
  text-align: center;
  line-height: 0.8rem;
  border: 1px solid #d6d6d6;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
.num-bar input {
  display: inline-block;
  vertical-align: middle;
  width: 1.0666666667rem;
  margin: 0 0.1333333333rem;
  height: 0.8rem;
  font-size: 0.35rem;
  text-align: center;
  border: 1px solid #d6d6d6;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
.check-box {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
}
.check-box .wrap {
  display: inline-block;
  position: relative;
  color: #e4e4e4;
  font-size: 0.3733333333rem;
}
.check-box input {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 9;
  left: 0;
  top: 0;
  opacity: 0;
}
.check-box i.fa-check-circle {
  color: #e94045;
}
