/* base style */
* {
  margin: 0;
  padding: 0;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
@font-face {
  font-family: "medium";
  src: url('../fonts/medium.ttf') format('truetype');
}
html {
  height: 100%;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
body {
  font-family: Helvetica, "PingFang-SC-Medium", "Helvetica Neue", "微软雅黑", Tahoma, Arial, sans-serif;
  font-size: 14px;
  line-height: normal;
  color: #333;
  height: 100%;
}
[v-cloak] {
  display: none;
}
a {
  color: #333;
}
a,
img {
  border: 0;
  outline: none;
  text-decoration: none;
}
a:focus,
a:hover {
  text-decoration: none;
  color: #333;
}
li {
  list-style: none;
}
p {
  margin: 0;
}
br {
  height: 0;
  width: 0;
  line-height: 0;
}
div,
p,
span,
a {
  line-height: 100%;
}
.grey-bg {
  background-color: #F4F4F4 !important;
}
.white-txt {
  color: #fff;
}
.red-txt {
  color: #c61a1a;
}
.grey-txt {
  color: #999;
}
.height-full {
  height: 100%;
}
.clear-fix:after,
.clear-fix:before {
  display: table;
  content: " ";
  clear: both;
  visibility: hidden;
  overflow: hidden;
}
input {
  border-style: none;
  background-color: transparent;
}
input:focus {
  outline: none;
}
select {
  border: solid 0 #000;
  appearance: none;
  background: none;
  -moz-appearance: none;
  -webkit-appearance: none;
}
select:focus {
  outline: none;
}
.table-wrap {
  display: table;
  table-layout: fixed;
  width: 100%;
}
.table-wrap .table-label {
  text-align: center;
  display: table-cell;
  vertical-align: middle;
}
.margin-0 {
  margin: 0 !important;
}
.readonly {
  border: 0;
  background-color: #ececec;
}
.mask-block {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.5);
}
.ui-blur {
  -webkit-filter: blur(5px);
  filter: blur(5px);
}
.hide {
  display: none !important;
}
.show {
  display: block !important;
}
