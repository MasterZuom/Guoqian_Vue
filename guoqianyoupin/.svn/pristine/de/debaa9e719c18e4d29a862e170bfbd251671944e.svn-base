.ui-align {
  text-align: center;
}
textarea {
  resize: none;
}
.float-right {
  float: right;
}
.float-left {
  float: left;
}
.score-page {
  padding-bottom: 9vw;
}
.score-page .card-view {
  position: relative;
  width: 92.5333vw;
  height: 39.4667vw;
  margin: 0 auto;
  color: #8A8A8A;
  background-color: #F4F4F4;
  background: url("../imgs/score/bg_integration@2x.png") no-repeat;
  background-size: 100%;
}
.score-page .card-view::after {
  content: "";
  position: absolute;
  left: 50%;
  top: 27vw;
  width: 1px;
  height: 5.6vw;
  background-color: #8A8A8A;
  -webkit-transform: scale(0.5, 1);
          transform: scale(0.5, 1);
}
.score-page .card-view .top {
  padding-top: 5.3333vw;
  font-size: 3.2vw;
  text-align: center;
}
.score-page .card-view .total-number {
  padding: 2.1vw 0 4vw 0;
  font-family: "medium";
  font-size: 13.3333vw;
  text-align: center;
  color: #fff;
}
.score-page .card-view .bottom {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  font-size: 4vw;
}
.score-page .card-view .bottom > div {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-flex: 1;
      -ms-flex: 1;
          flex: 1;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
}
.score-page .card-view .bottom .number {
  max-width: 38%;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.score-page .card-view .bottom .time {
  display: block;
  padding-top: 1.3333vw;
  font-size: 2.4vw;
}
.score-page .details .block-title {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  padding: 6.6667vw 0 3.7333vw 4vw;
  font-size: 4vw;
}
.score-page .details .block-title .icon {
  margin-right: 0.8vw;
  width: 3.8667vw;
  height: 3.8667vw;
  background: url("../imgs/score/icon_1.png") no-repeat;
  background-size: 100%;
}
.score-page .details .item {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: justify;
      -ms-flex-pack: justify;
          justify-content: space-between;
  height: 15.7333vw;
  padding: 0 4vw;
  font-size: 3.7333vw;
  border: 0.1333vw solid #f4f4f4;
}
.score-page .details .item .order-number {
  display: block;
  padding-bottom: 2.1333vw;
}
.score-page .details .item .time {
  font-size: 2.9333vw;
  color: #999;
}
.score-page .details .item .changes.add {
  color: #C61A1A;
}
