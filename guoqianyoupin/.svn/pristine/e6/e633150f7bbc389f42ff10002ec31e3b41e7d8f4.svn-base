  width: 92vw;
  margin: 0 auto;
  min-height: 88%;
}
.card-page .cards.unused .card-top,
.card-page .cards.unused .total {
  background: linear-gradient(-86deg, #24abc3, #24c3be);
}
.card-page .cards.used .card-top,
.card-page .cards.used .total {
  background: linear-gradient(-86deg, #6c6c6c, #a3a3a3);
}
.card-page .cards.used .txt,
.card-page .cards.used .rmb,
.card-page .cards.used .number {
  color: #999 !important;
}
.card-page .cards.used .label {
  background: url("../imgs/gift_card/used.jpg");
  background-size: 100%;
}
.card-page .cards.overdue .card-top,
.card-page .cards.overdue .total {
  background: linear-gradient(-86deg, #bcbcbc, #dadada);
}
.card-page .cards.overdue .txt,
.card-page .cards.overdue .rmb,
.card-page .cards.overdue .number {
  color: #CBCBCB !important;
}
.card-page .cards.overdue .describe-info {
