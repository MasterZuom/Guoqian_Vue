.share-module {
  .label-title{
    padding: 6.6667vw 0;
    font-size: 3.2vw;
    color: #666;
    &::before,&::after{
      width: 22.6667vw;
      color: #A6A6A6;
    }
  }
  .share-list{
    padding: 0 5.6vw;
    .flex-box();
    align-items: center;
    justify-content: space-between;
    .item{
      flex: 0 0 13.3333vw;
      text-align: center;
      img{
        width: 100%;
        height: auto;
      }
      .label{
        display: block;
        padding: 2.6667vw 0 4vw;
        font-size: 3.2vw;
      }
    }
  }
}