.ui-align {
  text-align: center;
}
textarea {
  resize: none;
}
.float-right {
  float: right;
}
.float-left {
  float: left;
}
.coupon-page {
  height: 100%;
  background: #F4F4F4;
}
.coupon-page .coupons {
  padding-top: 4.5333vw;
  width: 92vw;
  margin: 0 auto;
}
.coupon-page .coupons.unused .price {
  background: #24C3BE;
}
.coupon-page .coupons.unused .price::before {
  background-image: url("../imgs/coupon/bg_1.jpg");
}
.coupon-page .coupons.used .coupon-info .name {
  color: #999;
}
.coupon-page .coupons.used .price {
  background: #999;
}
.coupon-page .coupons.used .price::before {
  background-image: url("../imgs/coupon/bg_2.jpg");
}
.coupon-page .coupons.used .label {
  background: url("../imgs/coupon/used.jpg");
  background-size: 100%;
}
.coupon-page .coupons.overdue .coupon-info .name {
  color: #999;
}
.coupon-page .coupons.overdue .price {
  background: #CCCBCB;
}
.coupon-page .coupons.overdue .price::before {
  background-image: url("../imgs/coupon/bg_3.jpg");
}
.coupon-page .coupons.overdue .label {
  background: url("../imgs/coupon/overdue.jpg");
  background-size: 100%;
}
.coupon-page .coupons .coupon-price-view {
  position: relative;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  height: 24.8vw;
}
.coupon-page .coupons .label {
  position: absolute;
  right: 0;
  top: 0;
  width: 13.7333vw;
  height: 13.7333vw;
}
.coupon-page .coupons .coupon-item {
  padding-bottom: 2.6667vw;
}
.coupon-page .coupons .price {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
  width: 33.8667vw;
  color: #fff;
  background: #24C3BE;
}
.coupon-page .coupons .price::before {
  content: "";
  position: absolute;
  display: block;
  width: 1.4667vw;
  height: 100%;
  background-size: contain;
  background-repeat: no-repeat;
  z-index: 1;
  left: 0;
  top: 0;
}
.coupon-page .coupons .price .value {
  font-size: 4vw;
}
.coupon-page .coupons .price .value .number {
  padding-right: 1.3333vw;
  font-size: 10.6667vw;
  font-family: "medium";
}
.coupon-page .coupons .price .describe {
  padding-top: 1.3333vw;
  font-size: 2.6667vw;
}
.coupon-page .coupons .coupon-info {
  -webkit-box-flex: 1;
      -ms-flex: 1;
          flex: 1;
  padding: 4.2667vw 2.6667vw 0 2.9333vw;
  background: #fff;
  border: 0.1333vw solid #EAEAEA;
}
.coupon-page .coupons .coupon-info .name {
  font-size: 3.7333vw;
}
.coupon-page .coupons .coupon-info .time {
  padding: 2.1333vw 0 3.4667vw;
}
.coupon-page .coupons .coupon-info .time,
.coupon-page .coupons .coupon-info .coupon-number {
  font-size: 2.6667vw;
  color: #999;
}
.coupon-page .coupons .coupon-info .bottom {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: justify;
      -ms-flex-pack: justify;
          justify-content: space-between;
}
.coupon-page .coupons .coupon-info .btn {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
  width: 19.2vw;
  height: 5.8667vw;
  text-align: center;
  font-size: 3.2vw;
  border-radius: 2.9333vw;
  border: 1px solid #ccc;
}
